{
  "name": "magicui",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "scripts": {
    "prefix:cli": "pnpm --prefix ./packages/cli/ -- ",
    "install:cli": "pnpm prefix:cli install",
    "dev:cli": "pnpm prefix:cli dev",
    "build:cli": "pnpm prefix:cli build",
    "release:cli": "pnpm prefix:cli release",
    "preinstall": "npx only-allow pnpm",
    "dev": "concurrently \"content-collections watch\" \"next dev --turbopack\"",
    "prebuild": "pnpm build:docs && pnpm build:registry",
    "build": "next build",
    "build:registry": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/build-registry.mts && prettier --loglevel silent --write \"registry/**/*.{ts,tsx,mdx}\" --cache",
    "create:component": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/create-new-component.mts",
    "delete:component": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/delete-component.mts",
    "build:docs": "content-collections build",
    "typecheck": "pnpm build:docs && tsc --noEmit",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "prepare": "husky && test -f .env.local || cp .env.example .env.local"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@nextui-org/react": "^2.6.11",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-hover-card": "^1.1.4",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@t3-oss/env-nextjs": "^0.7.3",
    "@types/lodash.template": "^4.5.3",
    "autoprefixer": "10.4.14",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.6.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "cobe": "^0.6.3",
    "concurrently": "^9.1.2",
    "crisp-sdk-web": "^1.0.25",
    "critters": "^0.0.24",
    "date-fns": "^2.30.0",
    "geist": "^1.3.1",
    "gray-matter": "^4.0.3",
    "jotai": "^2.11.0",
    "lodash.template": "^4.5.0",
    "lucide-react": "^0.401.0",
    "mitt": "^3.0.1",
    "motion": "^11.17.0",
    "next": "15.1.4",
    "next-themes": "^0.3.0",
    "octokit": "^4.1.0",
    "postcss": "8.4.39",
    "posthog-js": "^1.205.1",
    "react": "19.0.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "react-resizable-panels": "^2.1.7",
    "react-tweet": "^3.2.1",
    "schema-dts": "^1.1.2",
    "sonner": "^1.7.1",
    "tailwind-merge": "^2.6.0",
    "ts-morph": "^23.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@content-collections/cli": "^0.1.6",
    "@content-collections/core": "^0.7.3",
    "@content-collections/mdx": "^0.1.6",
    "@content-collections/next": "^0.2.4",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^20.17.12",
    "@types/react": "19.0.6",
    "@types/react-dom": "19.0.3",
    "@types/unist": "^3.0.3",
    "cssnano": "^6.1.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.1.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-tailwindcss": "3.13.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "mdast-util-toc": "^6.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "rehype": "^13.0.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.13.2",
    "rehype-slug": "6.0.0",
    "remark": "^14.0.3",
    "remark-code-import": "^1.2.0",
    "remark-gfm": "^4.0.0",
    "rimraf": "^5.0.10",
    "tailwindcss": "3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "unist-builder": "4.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "engines": {
    "node": ">=20.6.1",
    "pnpm": ">=9"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.0.6",
      "@types/react-dom": "19.0.3"
    }
  }
}
